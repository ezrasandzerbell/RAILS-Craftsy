<div class="well bgWhite">

  <%= form_for [@product, @review] do |f| %>
    <%= f.label :title,'Review Title' %>
    <%= f.text_field :title, :class => 'form-control' %>
    <%= f.label :content,'Write about the product here' %>
    <%= f.text_field :content, :class => 'form-control' %>
    <%= f.submit %>
  <% end %>

</div>
